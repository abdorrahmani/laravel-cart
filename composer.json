{
  "name": "abdorrahmani/laravel-cart",
  "description": "A simple and professional package for the cart in Laravel",
  "type": "library",
  "keywords": ["laravel", "cart"],
  "authors": [
    {
      "name": "Mohammad Abdorrahmani",
      "email": "mohammad02abdorrahmani@gmail.com",
      "homepage": "https://anophel.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "LaravelCart\\": "src/"
    }
  },
  "require": {
    "php": "^7.3|~8.0.0|~8.1.0|~8.2.0|~8.3.0",
    "laravel/framework": "^8.74|^9.0|^10.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.4"
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelCart\\CartServiceProvider"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelCart\\Tests\\": "tests/"
    }
  }
}
